{
  "name": "my-seed-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./public/build",
    "start": "nf start",
    "foreman": "nf start web=1 -j Procfile_dev -p 5000",
    "predev": "npm run lint",
    "dev": "parallelshell 'npm run foreman' 'npm run livereload' 'open http://dev.ian:5000'",
    "livereload": "live-reload --port 9091 .",
    "postinstall": "bower install",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "karma": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "test": "npm run test-single-run && npm run protractor",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "prebuildjs": "npm run lint && npm run test-single-run",
    "buildjs": "webpack -p",
    "lint": "jshint ./public/**/*.js"
  },
  "engines": {
    "node": "4.2.1"
  },
  "dependencies": {
    "body-parser": "~1.10.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.1.1",
    "ejs": "~1.0.0",
    "express": "~4.10.6",
    "less-middleware": "1.0.x",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0"
  },
  "devDependencies": {
    "jasmine-core": "2.3.4",
    "jshint": "2.9.1-rc1",
    "karma": "0.13.15",
    "karma-chrome-launcher": "0.2.1",
    "karma-firefox-launcher": "0.1.7",
    "karma-jasmine": "0.3.6",
    "karma-junit-reporter": "0.3.8",
    "live-reload": "1.1.0",
    "nodemon": "1.8.1",
    "opener": "1.4.1",
    "parallelshell": "2.0.0",
    "protractor": "2.5.1",
    "rimraf": "2.4.4",
    "shelljs": "0.5.3"
  }
}
