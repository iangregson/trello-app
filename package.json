{
  "name": "trello-app",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "clean": "rimraf ./public/build",
    "start": "nf start",
    "foreman": "nf start web=1 -j Procfile_dev -p 5000",
    "predev": "npm install && npm run lint",
    "dev": "parallelshell 'npm run foreman' 'npm run livereload'",
    "livereload": "live-reload --port 9091 .",
    "postinstall": "bower install",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "karma": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "test": "npm run unittest && npm run protractor",
    "unittest": "mocha && npm run test-single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "npm run foreman & sleep 3s && protractor e2e-tests/protractor.conf.js && nf stop",
    "prebuildjs": "npm run lint && npm run unittest",
    "buildjs": "webpack -p",
    "lint": "jshint ./public/**/*.js",
    "presave": "npm run unittest",
    "prepatch": "npm run unittest",
    "save": "git add . -A && git commit && git push -u github master",
    "patch": "git add . -A && git commit -m 'version patch' && npm version patch && git push -u github master && git push heroku master",
    "watchify": "watchify ./public/app/app.js --outfile ./public/build/bundle.js"
  },
  "engines": {
    "node": "4.2.1"
  },
  "dependencies": {
    "angular": "1.4.9",
    "angular-ui-router": "0.2.17",
    "body-parser": "~1.10.1",
    "bower": "1.4.2",
    "browserify": "13.0.0",
    "connect-flash": "0.1.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.1.1",
    "ejs": "~1.0.0",
    "express": "~4.10.6",
    "express-session": "1.12.1",
    "less-middleware": "1.0.x",
    "lodash": "4.0.1",
    "mongoose": "4.3.4",
    "morgan": "~1.5.1",
    "node-trello": "1.1.2",
    "oauth": "0.9.14",
    "passport": "0.3.2",
    "passport-trello": "0.1.5",
    "serve-favicon": "~2.2.0",
    "underscore": "1.8.3",
    "wagner-core": "0.2.0"
  },
  "devDependencies": {
    "browserify-ng-html2js": "1.1.5",
    "gulp": "3.9.0",
    "gulp-concat": "2.6.0",
    "gulp-ng-html2js": "0.2.2",
    "jasmine-core": "2.3.4",
    "jshint": "2.9.1-rc1",
    "karma": "0.13.15",
    "karma-chrome-launcher": "0.2.1",
    "karma-firefox-launcher": "0.1.7",
    "karma-jasmine": "0.3.6",
    "karma-junit-reporter": "0.3.8",
    "live-reload": "1.1.0",
    "mocha": "2.3.4",
    "nodemon": "1.8.1",
    "opener": "1.4.1",
    "parallelshell": "2.0.0",
    "protractor": "2.5.1",
    "rimraf": "2.4.4",
    "shelljs": "0.5.3",
    "should": "7.1.1",
    "supertest": "1.1.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0",
    "webpack": "1.12.9"
  }
}
